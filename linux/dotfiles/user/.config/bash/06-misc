# WSL2
if command grep -q 'microsoft-standard-WSL2' /proc/sys/kernel/osrelease; then
    function outclip () {
        [[ -t 0 ]] && builtin printf 'usage: <command> | outclip\n' && builtin return
        command clip.exe
    }
fi

